---
import Instagram from "../assets/icons/instagram.svg?raw"
import Github from "../assets/icons/github.svg?raw"
import LinkedIn from "../assets/icons/linkedin.svg?raw"
import GMail from "../assets/icons/gmail.svg?raw"
import Nexus from "../assets/icons/nexus.svg?raw"
import Left from "../assets/icons/left.svg?raw"

const IconMap = {
  instagram: Instagram,
  github: Github,
  linkedin: LinkedIn,
  gmail: GMail,
  nexus: Nexus,
  left: Left
}

interface Props {
  of: keyof typeof IconMap;
}

const {of} = Astro.props
---

<Fragment set:html={IconMap[of]}/>
